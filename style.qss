QWidget {
    background-color: #ffffff;
    font-weight: 500;
    font-size: 12px;
    color: #424242;
}

QPushButton {
    border: 1px solid #424242;
    border-radius: 4px;
    padding: 4px 16px;
    margin: 0px;
}

QLineEdit {
    border: 1px solid #424242;
    border-radius: 4px;
    padding: 2px 8px;
}

QComboBox {
    border: 1px solid #424242;
    border-radius: 4px;
    padding: 2px 8px;
}

QLabel {
    font-weight: 600;
    font-size: 14px;
}

